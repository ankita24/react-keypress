    "url": "https://github.com/zzarcon/react-keypress/issues"
  },
  "homepage": "https://github.com/zzarcon/react-keypress",
  "dependencies": {
    "exportable": "0.1.2"
  },
  "devDependencies": {
    "babel-core": "^5.4.3",
    "babel-loader": "^5.1.2",
    "http-server": "~0.7.1",
    "webpack": "~1.4.4",
    "webpack-dev-server": "~1.6.5"
  }
}
